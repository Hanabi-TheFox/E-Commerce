<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.Commande" table="commande" schema="e_commerce">
        <id name="idCommande">
            <column name="id_commande" sql-type="int"/>
        </id>
        <property name="prix">
            <column name="prix" sql-type="decimal(10,2)" precision="10" scale="2"/>
        </property>
        <property name="status">
            <column name="status" sql-type="varchar(50)" length="50"/>
        </property>
        <set name="commandeProduitsByIdCommande" inverse="true">
            <key>
                <column name="id_commande" not-null="true"/>
            </key>
            <one-to-many not-found="ignore" class="entity.CommandeProduit"/>
        </set>
    </class>
</hibernate-mapping>