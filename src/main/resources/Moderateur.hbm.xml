<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.Moderateur" table="moderateur" schema="e_commerce">
        <id name="idModerateur">
            <column name="id_moderateur" sql-type="int"/>
        </id>
        <property name="droits">
            <column name="droits" sql-type="varchar(5)" length="5"/>
        </property>
    </class>
</hibernate-mapping>