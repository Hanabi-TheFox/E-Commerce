<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="entity.Client">
        <table name="client" schema="e_commerce" catalog=""/>
        <attributes>
            <id name="idClient">
                <column name="id_client" precision="-1"/>
            </id>
            <basic name="idCommande">
                <column name="id_commande" nullable="false" precision="-1"/>
            </basic>
            <basic name="compteBancaireNum">
                <column name="compteBancaireNum" length="19"/>
            </basic>
            <basic name="compteBancaireSolde">
                <column name="compteBancaireSolde" nullable="false" precision="2"/>
            </basic>
            <basic name="droits">
                <column name="droits" length="5"/>
            </basic>
            <many-to-one name="commandeByIdCommande" optional="true" target-entity="entity.Commande">
                <join-column name="id_commande" nullable="false" referenced-column-name="id_commande"/>
            </many-to-one>
            <one-to-one name="utilisateurByIdClient" optional="true" target-entity="entity.Utilisateur">
                <join-column name="id_client" referenced-column-name="id_utilisateur"/>
            </one-to-one>
        </attributes>
    </entity>
    <entity class="entity.Client">
        <table name="client" schema="e_commerce" catalog=""/>
        <attributes>
            <id name="idClient">
                <column name="id_client" precision="-1"/>
            </id>
            <basic name="idCommande">
                <column name="id_commande" nullable="false" precision="-1"/>
            </basic>
            <basic name="compteBancaireNum">
                <column name="compteBancaireNum" length="19"/>
            </basic>
            <basic name="compteBancaireSolde">
                <column name="compteBancaireSolde" nullable="false" precision="2"/>
            </basic>
            <basic name="droits">
                <column name="droits" length="5"/>
            </basic>
            <many-to-one name="commandeByIdCommande" optional="true" target-entity="entity.Commande">
                <join-column name="id_commande" nullable="false" referenced-column-name="id_commande"/>
            </many-to-one>
            <one-to-one name="utilisateurByIdClient" optional="true" target-entity="entity.Utilisateur">
                <join-column name="id_client" referenced-column-name="id_utilisateur"/>
            </one-to-one>
        </attributes>
    </entity>
    <entity class="entity.Commande">
        <table name="commande" schema="e_commerce" catalog=""/>
        <attributes>
            <id name="idCommande">
                <column name="id_commande" precision="-1"/>
            </id>
            <basic name="prix">
                <column name="prix" precision="2"/>
            </basic>
            <basic name="status">
                <column name="status" length="50"/>
            </basic>
            <one-to-many name="commandeProduitsByIdCommande" mapped-by="commandeByIdCommande"
                         target-entity="entity.CommandeProduit"/>
        </attributes>
    </entity>
    <entity class="entity.CommandeProduit">
        <table name="commande_produit" schema="e_commerce" catalog=""/>
        <attributes>
            <id name="idCommandeProd">
                <column name="id_commandeProd" precision="-1"/>
            </id>
            <basic name="idCommande">
                <column name="id_commande" nullable="false" precision="-1"/>
            </basic>
            <basic name="idProduit">
                <column name="id_produit" nullable="false" precision="-1"/>
            </basic>
            <basic name="quantite">
                <column name="quantite" nullable="false" precision="-1"/>
            </basic>
            <many-to-one name="produitByIdProduit" optional="true" target-entity="entity.Produit">
                <join-column name="id_produit" nullable="false" referenced-column-name="id_produit"/>
            </many-to-one>
        </attributes>
    </entity>
    <entity class="entity.Produit">
        <table name="produit" schema="e_commerce" catalog=""/>
        <attributes>
            <id name="idProduit">
                <column name="id_produit" precision="-1"/>
            </id>
            <basic name="nom">
                <column name="nom" length="50"/>
            </basic>
            <basic name="prix">
                <column name="prix" precision="2"/>
            </basic>
            <basic name="stock">
                <column name="stock" nullable="false" precision="-1"/>
            </basic>
        </attributes>
    </entity>
    <entity class="entity.Utilisateur">
        <table name="utilisateur" schema="e_commerce" catalog=""/>
        <attributes>
            <id name="idUtilisateur">
                <column name="id_utilisateur" precision="-1"/>
            </id>
            <basic name="nom">
                <column name="nom" length="20"/>
            </basic>
            <basic name="prenom">
                <column name="prenom" length="20"/>
            </basic>
            <basic name="mail">
                <column name="mail" length="30"/>
            </basic>
            <basic name="motDePasse">
                <column name="motDePasse" length="20"/>
            </basic>
            <basic name="typeDeCompte">
                <column name="TypeDeCompte" precision="-1"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
